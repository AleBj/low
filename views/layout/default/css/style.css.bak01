/* ----- */
nav {
    display: flex;
    justify-content: flex-end;
    align-items: center;
    position: absolute;
    right: 0px;
    width:auto;
    margin-bottom: 14px;
}
nav > ul{
    list-style: none;
    display: flex;
    flex-direction: row;
    justify-content: center;
}
nav ul li{
    display: block;
}
nav ul li:first-child{
    margin-right: 20px;
}
nav ul li a{
    font-family: 'nexaregular', sans-serif;
    font-weight: 400;
    text-decoration: none;
    font-size: 19px;
    letter-spacing: 1px;
    color:#eeead4;
    text-transform: uppercase;
}
nav ul li a:hover{
    color:#ffffff;
    text-decoration: underline;
}
/**/
.menu__btn{
    height: 14px;
    width: 20px;
    display: flex;
    flex-direction: column;
    justify-content: center;
    margin-right: 100px;
    position: fixed;
    cursor: pointer;
    z-index: 100;
    top:60px;
    right: 50%;
    transition: 0.5s;
    -webkit-transition: 0.5s;
    transform: translateX(500px);
}

/* ----- */
.menu__btn span{
    height: 1.8px;
    width:100%;
    background:#ffffff;
    display: block;
    z-index: 10;
}
.menu__btn span:nth-child(2){
    margin: 5px 0;
}
.spanTop{
    animation: 0.5s spanTop forwards;
    -webkit-animation: 0.5s spanTop forwards;
    -moz-animation: 0.5s spanTop forwards;
    -o-animation: 0.5s spanTop forwards;
}
@keyframes spanTop {
    to{
        transform: rotate(45deg);
        width: 100%;
    }
}
@-webkit-keyframes spanTop {
    to{
        transform: rotate(45deg);
        width: 100%;
    }
}
@-moz-keyframes spanTop {
    to{
        transform: rotate(45deg);
        width: 100%;
    }
}
@-o-keyframes spanTop {
    to{
        transform: rotate(45deg);
        width: 100%;
    }
}
.spanMid{
    animation: 0.5s spanMid forwards;
    -webkit-animation: 0.5s spanMid forwards;
    -moz-animation: 0.5s spanMid forwards;
    -o-animation: 0.5s spanMid forwards;
}
@keyframes spanMid {
    to{
        opacity: 0;
        height: 0px;
        margin: 0 0;
    }
}
@-webkit-keyframes spanMid {
    to{
        opacity: 0;
        height: 0px;
        margin: 0 0;
    }
}
@-moz-keyframes spanMid {
    to{
        opacity: 0;
        height: 0px;
        margin: 0 0;
    }
}
@-o-keyframes spanMid {
    to{
        opacity: 0;
        height: 0px;
        margin: 0 0;
    }
}
.spanBot{
    margin-top: -2px;
    animation: 0.5s spanBot forwards;
    -webkit-animation: 0.5s spanBot forwards;
    -moz-animation: 0.5s spanBot forwards;
    -o-animation: 0.5s spanBot forwards;
}
@keyframes spanBot {
    to{
        transform: rotate(-45deg);
    }
}
@-webkit-keyframes spanBot {
    to{
        transform: rotate(-45deg);
    }
}
@-moz-keyframes spanBot {
    to{
        transform: rotate(-45deg);
    }
}
@-webkit-keyframes spanBot {
    to{
        transform: rotate(-45deg);
    }
}

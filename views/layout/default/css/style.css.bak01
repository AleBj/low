}

/* ----- */
.read-blog__row{
    display: flex;
    justify-content: space-between;
    align-items: baseline;
    margin-bottom: 30px;
}
article.card{
    width: 29%;
    max-width: 360px;
    min-height: 429px;
    overflow: hidden;
    border-radius: 3px;
    background:#f2f0e2;
    box-shadow: 0px 0px 10px rgba(0,0,0,0.3);
}
a.card__img{
    width: 100%;
    height: 20vw;
    max-height: 285px;
    overflow: hidden;
    object-fit: contain;
    border-radius: 0 0 3px 3px;
    display: flex;
    justify-content: center;
    align-items: center;
    background:/*#0a263c*/#ffffff;
}
a.card__img::before{
    content:'';
    display: none;
    position: absolute;
    z-index: 2;
    height: 80px;
    width: 80px;    
    background:rgba(0, 0, 0, 0.65);
    background-image: url(../img/share-solid.png);
    background-repeat: no-repeat;
    background-size: cover;
    background-position: center;
    background-size: 20px 20px;
    border-radius:50% ;
    transition:all 0.5s ease-in-out
}
a.card__img:hover img{
    opacity: 0.5;
    transition:all 0.5s ease-in-out
}
a.card__img:hover::before{
    display: block;
    transition:all 0.5s ease-in-out;
    transform: rotate(-45deg);
    animation:0.5s cardArrow forwards;
}
@keyframes cardArrow {
    0%{
        transform:rotate(-45deg);
        opacity: 0;
    }
    75%{
        transform:rotate(25deg);
    }
    100%{
        transform:rotate(0deg);
        opacity: 1;
    }
}
.card__img img{
    height: 100%;
    min-width: 100%;
}
.card__content{
    padding:16px;    
}
.card__content--tit a{
    font-size: 17px;
    color:#1e2b36;
}
.card__content--date{
    min-height: 45px;
    margin-bottom: 10px;
    display: flex;
    align-items: center;
    font-size: 16px;
    color:#919191;    
}
.card__content--down{
    font-size: 18px;
    line-height: 24px;
    margin-bottom: 10px;
}
.card__content--more{
    font-family: 'gothamregular', sans-serif;
    font-size: 14px;
    text-transform: uppercase;
    color:#517f96;
}
@media(max-width:1024px){
    section.read-blog .container > h3{

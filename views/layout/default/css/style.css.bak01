section.steps{
    background:#517f96;
    padding:10px;
    position: relative;
    height: auto;
    z-index: 0;    
}
section.steps .container{
    padding:70px 0;
    color:#ffffff;
}
.border-line{
    border: 1px solid #ffffff;
    display: block;
}
.steps__intro{    
    font-size: 21px;
    text-align: center;
    padding:0 0.12rem;
    max-width: 825px;
    margin: auto;
}
.steps__by-steps{
    display: grid;
    grid-template-columns: repeat(3,1fr);
    column-gap: 1%;
    text-align: center;
    font-size: 24px;
    margin-top: 50px;
}

[class ^= steps__by-steps--step]{
    height: 200px;
    display: flex;
    align-items: center;    
    justify-content: center;
    position: relative;
}
[class ^= steps__by-steps--step] p{
    z-index: 2;
}
[class ^= steps__by-steps--step] .digit{
    font-family: 'nexabold';
    font-size: 10em;
    text-align: center;
    margin: auto;
    width: 100%;
    display: block;
    position: absolute;
    color:#a78f60;
    margin-top: 35px;    
    z-index: 1;
}

}
.subir svg path{
    fill:#517f96;
}
.subir:hover{
    background:#ffffff;
    opacity:0.5;
    transition: all 0.15s ease-in-out;
    -webkit-transition: all 0.15s ease-in-out;
}
.subir:hover svg{
    fill:#162536;
}
.hidden{
    opacity:0;
    visibility: hidden;
}


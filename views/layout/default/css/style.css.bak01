/* ----- menu desplegable -----*/
.burguer-menu{
    width: 100%;
    height: 100%;
    background: rgba(22, 37, 54,0.8);
    position: fixed;
    top:0;
    bottom:0;
    z-index: 3;    
    display: none;
    transition: all 0.5s ease-in-out;
    -webkit-transition: all 0.5s ease-in-out;
}
.burguer-menu ul{
    list-style: none;
    width: 100%;
    padding:60px 20%;
    background: rgba(105, 185, 219, 0.65);
    display: grid;
    grid-template-columns: repeat(3,1fr);
    flex-wrap: nowrap;
    position: fixed;
    bottom:0;
    margin-bottom: 0;
    transform: translateY(100%);
    animation:1s footAppear forwards;
    -webkit-animation:1s footAppear forwards;
    -moz-animation:1s footAppear forwards;
    -o-animation:1s footAppear forwards;
}
.burguer-menu ul li{
    transition: 0.2s;
    -webkit-transition: 0.2s;
}
.burguer-menu ul li a{
    display: block;
    width: 100%;
    height: 77px;
    padding:10px;
    text-align: center;
    color:#ffffff;
    font-size: 22px;
    text-transform: uppercase;
    transition: all 0.15s ease-in-out;
    -webkit-transition: all 0.15s ease-in-out;
    opacity:0;
    animation: 0.4s footLinkShow forwards;
    -webkit-animation: 0.4s footLinkShow forwards;
    -moz-animation: 0.4s footLinkShow forwards;
    -o-animation: 0.4s footLinkShow forwards;
}
.burguer-menu ul li:first-child a{
    animation-delay: 1000ms;
    -webkit-animation-delay: 1000ms;
    -moz-animation-delay: 1000ms;
}
.burguer-menu ul li:nth-child(2) a{
    animation-delay: 1200ms;
    -webkit-animation-delay: 1200ms;
    -moz-animation-delay: 1200ms;
    -o-animation-delay: 1200ms;
}
.burguer-menu ul li:nth-child(3) a{
    animation-delay: 1400ms;
    -webkit-animation-delay: 1400ms;
    -moz-animation-delay: 1400ms;
    -o-animation-delay: 1400ms;
}
.burguer-menu ul li:nth-child(4) a{
    animation-delay: 1600ms;
    -webkit-animation-delay: 1600ms;
    -moz-animation-delay: 1600ms;
    -o-animation-delay: 1600ms;
}
.burguer-menu ul li:nth-child(5) a{
    animation-delay: 1800ms;
    -webkit-animation-delay: 1800ms;
    -moz-animation-delay: 1800ms;
    -o-animation-delay: 1800ms;
}
.burguer-menu ul li:nth-child(6) a{
    animation-delay: 2000ms;
    -webkit-animation-delay: 2000ms;
    -moz-animation-delay: 2000ms;
    -o-animation-delay: 2000ms;
}
.burguer-menu ul li:last-child a{
    animation-delay: 2200ms;
    -webkit-animation-delay: 2200ms;
    -moz-animation-delay: 2200ms;
    -o-animation-delay: 2200ms;
}
@keyframes footAppear{
    from{
        opacity: 0;
        transform: translateY(100%);
    }
    to{
        opacity: 1;
        transform: translateY(0%);
    }
}
@-webkit-keyframes footAppear{
    from{
        opacity: 0;
        transform: translateY(100%);
    }
    to{
        opacity: 1;
        transform: translateY(0%);
    }
}
@-moz-keyframes footAppear{
    from{
        opacity: 0;
        transform: translateY(100%);
    }
    to{
        opacity: 1;
        transform: translateY(0%);
    }
}
@-o-keyframes footAppear{
    from{
        opacity: 0;
        transform: translateY(100%);
    }
    to{
        opacity: 1;
        transform: translateY(0%);
    }
}
@keyframes footLinkShow{
    from{
        opacity: 0;
        transform: translateY(25%);
    }
    to{
        opacity: 1;
        transform: translateY(0%);
    }
}
@-webkit-keyframes footLinkShow{
    from{
        opacity: 0;
        transform: translateY(25%);
    }
    to{
        opacity: 1;
        transform: translateY(0%);
    }
}
@-moz-keyframes footLinkShow{
    from{
        opacity: 0;
        transform: translateY(25%);
    }
    to{
        opacity: 1;
        transform: translateY(0%);
    }
}
@-o-keyframes footLinkShow{
    from{
        opacity: 0;
        transform: translateY(25%);
    }
    to{
        opacity: 1;
        transform: translateY(0%);
    }
}

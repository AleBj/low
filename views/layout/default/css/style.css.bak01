/* ----- press page ----- */
section.press{
    padding: 50px 0;
    background: #ffffff;
}
.press__header{
    width: 100%;
    height: 100px;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #648ca8;
}
.press__header h3{
    font-size: 20px;
    color:#f2f0e2;
    text-align: center;
}
section.press p.intro{
    font-size: 20px;
    color:#454545;
    margin:17px 0;
    line-height: 1.5;
}
/* ----- press cards -----*/
section.porta-press{
    display: grid;
    grid-template-columns: repeat(2,1fr);
    grid-template-rows: auto;
    column-gap: 50px;
    row-gap: 50px;
    padding: 50px 0;
}
.press__card{
    border-radius: 3px;
    background: #f2f0e2;
    box-shadow: 0px 0px 10px rgba(0,0,0,0.3);
    display: flex;
    min-height: 25vh;
    max-height: 280px;
    width: 100%;
    max-width: 545px;
    cursor: pointer;
}
.press__card--img{
    overflow: hidden;
    margin: 22px;
    width: 40%;    
    height: auto;
    position: relative;
    background:#ffffff;
    display: flex;
    justify-content: center;
    align-items: center;    
}
.press__card--img img{
    position: absolute;    
    height: auto;
    width: 75%;    
}
.press__card--container{
    color:#5a6675;
    margin: 30px;
    width: 60%;
}
.press__card--tit{
    font-size: 16px;
    margin-bottom: 35px; 
    font-weight: bold;
}
.press__card--down{
    font-size: 16px;
    margin-bottom: 10px;
}
.press__card--date{
    color:#648ca8;
    font-weight: bold;
}
/* ----- */
.newstar{
    padding:70px;
    background: #fbfaf0;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.newstar p{
    font-size: 20px;
    text-align: center;
    color:#162536;
}
.newstar p:first-child{
    margin-bottom: 10px;
}
/* ----- */
.press__card--img::before{
    content:'';
    display: none;
    position: absolute;
    z-index: 2;
    height: 80px;
    width: 80px;    
    background:rgba(0, 0, 0, 0.65);
    background-image: url(../img/share-solid.png);
    background-repeat: no-repeat;
    background-size: cover;
    background-position: center;
    background-size: 20px 20px;
    border-radius:50% ;
    transition:all 0.5s ease-in-out
}
.press__card--img:hover::before{
    display: block;
    transition:all 0.5s ease-in-out;
    transform: rotate(-45deg);
    animation:0.5s cardArrow forwards;
}

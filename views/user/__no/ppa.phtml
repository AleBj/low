<script>
    let i = 0
    $('#btAddChild').on('click', function(){
        i++
        let clone = $('#child').clone()
        $('#duplicate-child').prepend(clone.attr('id','child'+i))
    })
    $('#btRemoveChild').on('click', function(){
        if(i > 0){
            $('#child'+i).remove()
            i--
        }
    })
    $('#navform li').on('click', function(){
        $('#navform li').removeClass('active')
        let data = $(this).data('section');
        $('.containerForm section').hide()
        $('.containerForm section#'+data).show()

        $(this).addClass('active')
    })
</script>



